{
  "name": "vscode-graphene",
  "displayName": "Graphene syntax highlighting",
  "description": "Syntax highlighting for the Graphene language",
  "publisher": "antroseco",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/antroseco/vscode-graphene/"
  },
  "contributes": {
    "languages": [
      {
        "id": "graphene",
        "aliases": [
          "Graphene",
          "graphene"
        ],
        "extensions": [
          ".c3"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "graphene",
        "scopeName": "source.graphene",
        "path": "./syntaxes/graphene.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.16.0"
  },
  "scripts": {
    "deploy": "vsce publish"
  }
}
